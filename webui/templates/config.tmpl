<table class="table table-striped table-bordered text-xsmall" id="config-table" style="font-size: 12px;">
    <thead>
      <tr>
        <th>Parameter</th>
        <th>Value</th>    
      </tr>
    </thead>
    <tbody>
        {{ range $key, $value := . }}
        <tr>
          <td>{{$key}}</td>
          <td>{{$value}}</td>
        </tr>
        {{end}}
    </tbody>
</table>